[user]
	name = Gabriel Musskopf
[init]
	defaultBranch = main
[alias]
    st = status
	co = checkout
	cb = checkout -b
    cp = cherry-pick
    aa = add --all
    pr = pull --rebase
    ss = stash save
    sp = stash pop
    cm = commit -m
    ca = commit --amend --no-edit
    cam = commit --amend
    pf = push --force-with-lease
[core]
	editor = nvim
[branch]
	sort = -committerdate
[column]
	ui = auto
[maintenance]
	repo = /home/gabrielmusskopf/dev/work/hypera/hypera-ph-catalog-service
